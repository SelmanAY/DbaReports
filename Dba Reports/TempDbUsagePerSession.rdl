  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with hspace_usage as
(
select 
	session_id	
	, database_id 
	, user_objects_alloc_page_count * 8 / 1024 as UserAllocations_MB
	, internal_objects_alloc_page_count * 8 / 1024 as InternalAllocations_MB
	, user_objects_dealloc_page_count * 8 / 1024 as UserDeAllocations_MB
	, internal_objects_dealloc_page_count * 8 / 1024 as InternalDeAllocations_MB
	, user_objects_deferred_dealloc_page_count * 8 / 1024 as UserDeferredDeAllocations_MB
from 
	sys.dm_db_session_space_usage
)
select 
	h.session_id
	, h.UserAllocations_MB
	, h.InternalAllocations_MB
	 , h.UserDeAllocations_MB
	 , h.InternalDeAllocations_MB
	 , h.UserDeferredDeAllocations_MB
	 , h.InternalAllocations_MB + h.UserAllocations_MB as TotalAllocations_MB
	 , h.InternalDeAllocations_MB + h.UserDeAllocations_MB as TotalDeAllocations_MB
	, s.host_name
	, s.program_name
	, s.login_name
	 , s.status
	 , s.cpu_time
	 , s.memory_usage
	 , s.total_scheduled_time
	 , s.total_elapsed_time
	 , s.reads
	 , s.writes
	 , s.logical_reads
	 , DB_NAME(r.database_id) as DatabaseName
	 , t.text
from hspace_usage as h
	inner join sys.dm_exec_sessions s on s.session_id = h.session_id
	 left join sys.dm_exec_requests r on r.session_id = s.session_id
	 outer apply sys.fn_get_sql(r.sql_handle) as t
where
	h.InternalAllocations_MB + h.UserAllocations_MB &gt; 100
order by h.InternalAllocations_MB + h.UserAllocations_MB desc</CommandText>
      </Query>
